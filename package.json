{
    "name": "puppeteer-on-lambda",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "tsc && node dist/index.js",
        "zip": "tsc && 7z a -r function.zip ./dist/*  node_modules/",
        "updateLamda": "npm run zip && aws s3 cp function.zip s3://herbhub-function && aws lambda update-function-code --function-name herbhub-data-collection --s3-bucket herbhub-function --s3-key function.zip && rm function.zip"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^14.14.13",
        "@types/puppeteer": "^5.4.2",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.252.0",
        "chrome-aws-lambda": "^10.1.0",
        "puppeteer-core": "^10.0.0",
        "puppeteer-extra": "^3.3.4",
        "puppeteer-extra-plugin-stealth": "^2.11.1"
    }
}
